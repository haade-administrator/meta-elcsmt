	rk809_sound: rk809-sound {
		status = "okay";
		compatible = "rockchip,multicodecs-card";
		rockchip,card-name = "rockchip-rk809";
		rockchip,format = "i2s";
		rockchip,mclk-fs = <256>;
		rockchip,cpu = <&i2s1_8ch>;
		rockchip,codec = <&rk809_codec>;
	};
	
	vad_sound: vad-sound {
		status = "disabled";
		compatible = "rockchip,multicodecs-card";
		rockchip,card-name = "rockchip,rk3568-vad";
		rockchip,cpu = <&i2s1_8ch>;
		rockchip,codec = <&rk809_codec>, <&vad>;
	};

	pdm_mic_array: pdm-mic-array {
		status = "okay";
		compatible = "simple-audio-card";
		simple-audio-card,name = "rockchip,pdm-mic-array";
		simple-audio-card,cpu {
			 sound-dai = <&pdm>;
		};
		simple-audio-card,codec {
			 sound-dai = <&pdmics>;
		};
	};

	pdmics: dummy-codec {
		status = "okay";
		compatible = "rockchip,dummy-codec";
		#sound-dai-cells = <0>;
	};
 
	/*rk_headset: rk-headset {
		compatible = "rockchip_headset";
		headset_gpio = <&gpio4 RK_PA1 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&hp_det>;
		io-channels = <&saradc 2>;
	};*/

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		clocks = <&rk809 1>;
		clock-names = "ext_clock";
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_enable_h>;

		/*
		 * On the module itself this is one of these (depending
		 * on the actual card populated):
		 * - SDIO_RESET_L_WL_REG_ON
		 * - PDN (power down when low)
		 */
		post-power-on-delay-ms = <200>;
		reset-gpios = <&gpio2 RK_PB1 GPIO_ACTIVE_LOW>;
	};

	vcc_sd: vcc-sd {
		compatible = "regulator-gpio";
		enable-active-low;
		enable-gpio = <&gpio0 RK_PA5 GPIO_ACTIVE_LOW>;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc_sd_h>;
		regulator-name = "vcc_sd";
		states = <3300000 0x0
			3300000 0x1>;
	};
	
	vcc_lcd: vcc-lcd-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc_lcd";
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		//enable-active-high;
		enable-active-low;
		//regulator-always-on;
		gpio = <&gpio0 RK_PA4 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_enable_gpio>;
		vin-supply = <&vcc_3v3>;

		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	wireless-wlan {
		compatible = "wlan-platdata";
		rockchip,grf = <&grf>;
		wifi_chip_type = "ap6398s";
		WIFI,poweren_gpio = <&gpio2 RK_PB1 GPIO_ACTIVE_HIGH>;
		WIFI,host_wake_irq = <&gpio2 RK_PB2 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_host_wake_irq>;
		status = "okay";
	};

	wireless-bluetooth {
		compatible = "bluetooth-platdata";
		clocks = <&rk809 1>;
		clock-names = "ext_clock";
		//wifi-bt-power-toggle;
		uart_rts_gpios = <&gpio2 RK_PB5 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default", "rts_gpio";
		pinctrl-0 = <&uart1m0_rtsn>,<&bt_reset_gpio>,<&bt_wake_gpio>,<&bt_irq_gpio>;
		pinctrl-1 = <&uart1_gpios>;
		BT,reset_gpio    = <&gpio2 RK_PB7 GPIO_ACTIVE_HIGH>;
		BT,wake_gpio     = <&gpio2 RK_PC1 GPIO_ACTIVE_HIGH>;
		BT,wake_host_irq = <&gpio2 RK_PC0 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
	
	//add by rocky
	test-power {
		status = "okay";
	};
	
	io_control {
		compatible = "io_control";
		gpio-one = <&gpio0 RK_PC6 GPIO_ACTIVE_LOW>;
		gpio-two = <&gpio0 RK_PC7 GPIO_ACTIVE_LOW>;
		gpio-three = <&gpio3 RK_PC6 GPIO_ACTIVE_LOW>;
		gpio-four = <&gpio3 RK_PC7 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default","one","two","three";
		pinctrl-0 = <&gpio_one>;
		pinctrl-1 = <&gpio_two>;
		pinctrl-2 = <&gpio_three>;
		pinctrl-3 = <&gpio_four>;
		status = "okay";
	};
	
	pwmleds{
		compatible = "pwm-wa-leds";
		pwms = <&pwm10 0 100000  0>;
		red-pwm-id = <10>;
		red-period = <1000000>;//100000
		red-default-level = <0>;
		green-pwm-id = <0>;//1
		green-period = <1000000>;//100000	
		green-default-level = <0>;
		blue-pwm-id = <2>;//5
		blue-period = <1000000>;//100000
		blue-default-level = <0>;

	        led_con_1 = <&gpio4 RK_PB1 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&led_pwr>;	
		status = "okay";
	};
	
	/*led_con_h {
		compatible = "led_con_h";
		//zigbee-en-gpio = <&gpio0 GPIO_C2 GPIO_ACTIVE_HIGH>;
		//led-power = <131>;//gpio4 RK_PA3
		led-gpio = <&gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;
		status = "disabled";
	};*/
