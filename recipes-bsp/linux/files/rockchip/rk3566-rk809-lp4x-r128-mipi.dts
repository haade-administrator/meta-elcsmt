   vcc5v0_rfid: vcc5v0-rfid {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio4 RK_PA2 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc5v0_rfid_en>;
		regulator-name = "vcc5v0_rfid";
		regulator-always-on;
		regulator-boot-on;
