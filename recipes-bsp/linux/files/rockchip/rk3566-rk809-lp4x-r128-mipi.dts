/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/display/media-bus-format.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/display/rockchip_vop.h>
#include "rk3566.dtsi"
//#include "rk3566-evb.dtsi"
#include "rk3568-evb-r128.dtsi"//rocky
#include "rk3568-linux.dtsi"
#include "lcd-JL101-CC1001-27K-r128.dtsi"
/ {
	model = "Rockchip RK3566 EVB2 LP4X V10 Board";
	compatible = "rockchip,rk3566-evb2-lp4x-v10", "rockchip,rk3566";

	adc_keys: adc-keys {
		compatible = "adc-keys";
		io-channels = <&saradc 0>;
		io-channel-names = "buttons";
		keyup-threshold-microvolt = <1800000>;
		poll-interval = <100>;

		vol-up-key {
			label = "volume up";
			linux,code = <KEY_VOLUMEUP>;
			press-threshold-microvolt = <1750>;
		};

		vol-down-key {
			label = "volume down";
			linux,code = <KEY_VOLUMEDOWN>;
			press-threshold-microvolt = <446000>;
		};
	};
	
	hdmi_sound: hdmi-sound {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,mclk-fs = <128>;
		simple-audio-card,name = "rockchip,hdmi";
		status = "disabled";

		simple-audio-card,cpu {
				sound-dai = <&i2s0_8ch>;
		};
		simple-audio-card,codec {
				sound-dai = <&hdmi>;
		};
	};

	
	
	dc_12v: dc-12v {
		compatible = "regulator-fixed";
		regulator-name = "dc_12v";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;//12000000
		regulator-max-microvolt = <12000000>;//12000000
	};
	
	vcc3v3_sys: vcc3v3-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&dc_12v>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&dc_12v>;
	};
	
	vcc_wifi: vcc-wifi-regulator {
		compatible = "regulator-fixed";
		gpio = <&gpio0 RK_PB7 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_bat>;
		regulator-name = "vcc_wifi";
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
	};
        vcc_pcm: vcc-pcm-regulator {
                compatible = "regulator-fixed";
                gpio = <&gpio2 RK_PC5 GPIO_ACTIVE_HIGH>;
                pinctrl-names = "default";
                pinctrl-0 = <&pcm_bat>;
                regulator-name = "vcc_pcm";
                enable-active-high;
                regulator-always-on;
                regulator-boot-on;
        };
	camera_pwdn:camera-pwdn-regulator {
        compatible = "regulator-fixed";
            regulator-name = "camera_pwdn";
            gpio = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
            pinctrl-names = "default";
            pinctrl-0 = <&camera_pwdns>;
            enable-active-high;
            regulator-always-on;
            regulator-boot-on;
    };
    mycamera_power:mycamera-power-regulator {
        compatible = "regulator-fixed";
            regulator-name = "mycamera_power";
            gpio = <&gpio3 RK_PD5 GPIO_ACTIVE_HIGH>;
            pinctrl-names = "default";
            pinctrl-0 = <&mycamera_powers>;
            enable-active-high;
            regulator-always-on;
            regulator-boot-on;
    };
	
	
	vcc_4g: vcc-4g-regulator {
		compatible = "regulator-fixed";
		gpio = <&gpio4 RK_PA4 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&pwr_4g>;
		regulator-name = "vcc_4g";
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
	};
	
	/*vcc_spk_con: vcc-spk-con {
		compatible = "regulator-fixed";
		gpio = <&gpio4 RK_PA3 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&spk_con>;
		regulator-name = "spk_con";
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
	};*/
	
	
	
